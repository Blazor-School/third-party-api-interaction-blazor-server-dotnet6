@page "/interfere-with-http-client-wrapper"
@using ThirdPartyApiInteraction.Utils
@inject InterfereByHttpClientWrapperClass HttpClientWrapper

<h3>InterfereWithHttpClientWrapper</h3>
<div>API result: @ApiResult</div>

@code {
    public string ApiResult { get; set; } = "";

    protected override async Task OnInitializedAsync() => ApiResult = await HttpClientWrapper.GetWeatherDataAsync();
}